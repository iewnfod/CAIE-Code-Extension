{
  "name": "caie-pseudocode-language-support",
  "displayName": "CAIE Pseudocode Language Support",
  "description": "A VSCode language support extension of CAIE PseudoCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cpc",
      "aliases": ["CAIE PseudoCode", "cpc"],
      "extensions": [".cpc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cpc",
      "scopeName": "source.cpc",
      "path": "./syntaxes/cpc.tmLanguage.json"
    }]
  }
}
